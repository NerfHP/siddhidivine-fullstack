{
  "name": "divine-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postinstall": "prisma generate --schema=./api/prisma/schema.prisma",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "echo 'No client tests configured yet'",
    "db:migrate": "npx dotenv -e .env.local -- npx prisma migrate dev --schema=./api/prisma/schema.prisma",
    "db:migrate:deploy": "npx dotenv -e .env.local -- npx prisma migrate deploy --schema=./api/prisma/schema.prisma",
    "db:migrate:status": "npx dotenv -e .env.local -- npx prisma migrate status --schema=./api/prisma/schema.prisma",
    "db:migrate:reset": "npx dotenv -e .env.local -- npx prisma migrate reset --schema=./api/prisma/schema.prisma",
    "db:push": "npx dotenv -e .env.local -- npx prisma db push --schema=./api/prisma/schema.prisma",
    "db:generate": "npx dotenv -e .env.local -- npx prisma generate --schema=./api/prisma/schema.prisma",
    "db:studio": "npx dotenv -e .env.local -- npx prisma studio --schema=./api/prisma/schema.prisma",
    "db:seed": "npx dotenv -e .env.local -- npx ts-node --esm ./api/prisma/seed.ts"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
